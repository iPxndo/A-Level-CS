using System;
using System.Collections.Generic;

public class Program
{
	public static void Main()
	{
		//create a 1D array with 5 places for accounts 
		Account[] Bank = new Account[5]; 
		//Rob's Account 
		Bank[0].Name = "Rob"; 
		Bank[0].Address = "Rob's House"; 
		Bank[0].Balance = 1000; 
		Bank[0].AccountNumber = 709831;
		
		//Jim's Account 
		Bank[1].Name = "Jim"; 
		Bank[1].Address = "Jim's House"; 
		Bank[1].Balance = 99; 
		Bank[1].AccountNumber = 792754;
		
		//Sandy's Account
		Bank[2].Name = "Sandy"; 
		Bank[2].Address = "Sandy's House"; 
		Bank[2].Balance = 100000; 
		Bank[2].AccountNumber = 38245;
		
		//Output accounts 
		PrintAccount(Bank[2]);
		PrintAccount(Bank[1]); 
		PrintAccount(Bank[0]);
	}
	
	public struct Account 
	{ 
		public string Name ; 
		public string Address ; 
		public int AccountNumber ; 
		public int Balance ; 
	}
	
	public static void PrintAccount(Account record) 
	{ 
		Console.WriteLine("Name: " + record.Name); 
		Console.WriteLine("Address: " + record.Address); 
		Console.WriteLine("Balance: " + record.Balance); 
		Console.WriteLine("AccountNumber: "+ record.AccountNumber + "\n"); 
	}
}
