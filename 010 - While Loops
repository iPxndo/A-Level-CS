using System;
					
public class Program
{
	public static void Main()
	{
		int x = 10;
		while(x > -1)
		{
			if(x == 0)
			{
				Console.WriteLine($"Rocket Launched!");
				x = x-1;
			}
			
			else if(x == 1)
			{
				Console.WriteLine($"The rocket will launch in {x} second");
				x = x-1;
			}
			
			else
			{
				Console.WriteLine($"The rocket will launch in {x} seconds");
				x = x-1;
			}
			
				
		}
	}
}
